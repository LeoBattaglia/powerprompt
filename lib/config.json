{
    "ansi": {
        "clearEntireLine":      "\u001b[2K",
        "clearEntireScreen":    "\u001b[2J",
        "clearLineToBegin":     "\u001b[1K",
        "clearLineToEnd":       "\u001b[0K",
        "clearScreenToBegin":   "\u001b[1J",
        "clearScreenToEnd":     "\u001b[0J",
        "down":                 "\u001b[{n}B",
        "left":                 "\u001b[{n}D",
        "nextLine":             "\u001b[{n}E",
        "previousLine":         "\u001b[{n}F",
        "reset":                "\u001b[0m",
        "restorePosition":      "\u001b[{u}",
        "right":                "\u001b[{n}C",
        "savePosition":         "\u001b[{s}",
        "setColumn":            "\u001b[{n}G",
        "setPosition":          "\u001b[{n};{m}H",
        "up":                   "\u001b[{n}A"
    },
    "colors": {
        "black":                "\u001b[30m",
        "blue":                 "\u001b[34m",
        "cyan":                 "\u001b[36m",
        "green":                "\u001b[32m",
        "magenta":              "\u001b[35m",
        "red":                  "\u001b[31m",
        "white":                "\u001b[37m",
        "yellow":               "\u001b[33m",

        "command":              "\u001b[32m",
        "default":              "\u001b[37m",
        "error":                "\u001b[31m",
        "input":                "\u001b[37m",
        "line":                 "\u001b[37m",
        "option":               "\u001b[33m",
        "selected":             "\u001b[36m",
        "title":                "\u001b[35m"
    },
    "terminal": {
        "length":               80
    }
}